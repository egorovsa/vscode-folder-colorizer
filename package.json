{
  "name": "folder-color",
  "publisher": "SergeyEgorov",
  "displayName": "FolderColorizer",
  "description": "The Folder Colorizer extension for Visual Studio Code provides an easy and convenient way to change the color of folders in the file explorer",
  "version": "0.0.6",
  "repository": "https://github.com/egorovsa/vscode-folder-colorizer",
  "icon": "./icon.png",
  "author": {
    "email": "egorovsa2@gmail.com",
    "name": "SergeyEgorov"
  },
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "menus": {
      "explorer/context": [
        {
          "command": "folder-color.setColor"
        },
        {
          "command": "folder-color.setBadge"
        },
        {
          "command": "folder-color.clearColorizer"
        }
      ]
    },
    "commands": [
      {
        "command": "folder-color.setColor",
        "title": "Colorize"
      },
      {
        "command": "folder-color.setBadge",
        "title": "Set budge"
      },
      {
        "command": "folder-color.clearColorizer",
        "title": "Clear colorizer"
      }
