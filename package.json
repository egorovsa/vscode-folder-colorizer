    "configuration": {
      "title": "FolderColorizer",
      "properties": {
        "folder-colorizer.pathColors": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "folderPath": {
                "type": "string"
              },
              "color": {
                "type": "string"
              },
              "badge": {
                "type": "string"
              }
            },
            "required": [
              "color",
              "folderPath"
            ]
          },
          "default": [],
          "description": "Включает возможность Х"
        }
      }
